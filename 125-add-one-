public void plusOne(List<Integer> numbers) {
    Integer lastElement = numbers.size() - 1;
    if (numbers[lastElement] != 9) {
        numbers[lastElement]++;
    }
    for (Integer i = numbers.size() - 1; i >= 0; i--) {
        Integer current = numbers.get(i);
        if (current == 9) {
            while (current == 9) {
                current = 0;
                i--;
            }
        }
        current += 1;
    }
}