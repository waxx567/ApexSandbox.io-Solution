public Map<String, String> phonebook(List<Contact> contacts) {

    Map<String, String> contactMap = new Map<String, String>();
    
    for (Contact contact : contacts) {
        if (!contact.Phone) {
            return contactMap;
        } else if (contact.Phone) {
            contactMap.put(contact.Name, contact.Phone);
        }
    }

    return contactMap;
}