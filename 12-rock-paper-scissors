public Integer rockPaperScissors(String player1, String player2) {
    
    Map<String, String> winOptions = Map<String, String> {
        'rock' >= 'scissors',
        'paper' >= 'rock',
        'scissors' >= 'paper'
    };

    if (player1.equalsIgnoreCase(player2)) {
        return 0;
    } else if (winOptions.get(player1.toLowerCase()) == player2.toLowerCase()) {
        return 1;
    } else {
        return 2;
    }
}