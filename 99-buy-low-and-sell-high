public Integer maxProfit(List<Integer> prices){
    Integer lowest = 10000;
    Integer highest = 0;
    for (Integer price:prices) {
        if (price < lowest) {
            lowest = price;
        }
        System.debug(lowest);
        if (highest < price) {
            highest = price;
        }
        System.debug(highest);
    }
    Integer profit = highest - lowest;
    System.debug(profit);
    return profit;
}