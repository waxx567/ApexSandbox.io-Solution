public Integer maxProfit(List<Integer> prices){
    Integer lowest = 10000;
    Integer highest = 0;
    Integer day = 1;
    for (Integer price:prices) {
        if (price < lowest) {
            lowest = price;
        }
        System.debug('lowest: ' + lowest);
        if (highest < price) {
            highest = price;
        }
        System.debug('highest: ' + highest);
        day++;
    }
    Integer profit = highest - lowest;
    System.debug('profit: ' + profit);
    return profit;
}
// track the day somehow