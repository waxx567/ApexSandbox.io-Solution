public Integer maxProfit(List<Integer> prices){
    Integer lowest = 10000;
    Integer highest = 0;
    Integer day = 1;
    Integer highestDay = 0;
    for (Integer price:prices) {
        System.debug('day: ' + day);
        System.debug('highestDay: ' + highestDay);
        if (highestDay > day) {
            if (price < lowest) {
                lowest = price;
            }
        }
        System.debug('lowest: ' + lowest);
        if (highest < price) {
            highest = price;
            highestDay = day;
        }
        System.debug('highest: ' + highest);
        day++;
    }
    Integer profit = highest - lowest;
    System.debug('profit: ' + profit);
    return profit;
}