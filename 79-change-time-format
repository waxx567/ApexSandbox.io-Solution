public String changeTimeFormat(String strTime) {

    List<String> time = strTime.split(':');

    Integer intHour = Integer.valueOf(time(0));

    if (intHour <= 12) {
        String lowHour = String.valueOf(intHour.format());
        return lowHour + ':' + mins + ' AM';
    }
    else if (intHour > 12) {
        intHour = intHour - 12;
        String highHour = String.valueOf(intHour.format());
        return highHour + ':' + mins + ' PM'; 
    }


}