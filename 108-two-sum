public static List<Integer> twoSum(List<Integer> nums, Integer target) {
    List<Integer> indices = new List<Integer>();
    Integer n = 0;
    for (Integer i = 0; i < nums.size(); i++) {
        if (nums[i] < target) {
            n = nums[i];
            // i++;
            if (i == (target - n)) {
                indices.add(n);
                indices.add(i);
            } //else {
            //     i++;
            // }
        }
    }
    return indices;
}