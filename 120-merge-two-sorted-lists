public static List<Integer> mergeLists(List<Integer> list1, List<Integer> list2){
    List<Integer> output = new List<Integer>();

    Integer i = 0; // list1 pointer
    Integer j = 0; // list2 pointer

    while (list1.size() < list2.size()) {
        if (list1[i] < list2[j]) {
            output.add(list1[i]);
        } else {
            output.add(list1[j]);
        }
    }

    return output;
}