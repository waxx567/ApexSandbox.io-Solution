public List<sObject> getListOfsObject(List<Account> accounts, List<Contact> contacts)
{
    List<sObject> returnList = new List<sObject>();

    if (accounts == null && contacts == null) {
        return returnList;
    }

    // Create sObjets for the inputs
    SObject account = new Account();
    for (Account a : accounts) {
        a = (Account)account;
    }
    SObject contact = new Contact();
    for (Contact c : contacts) {
        c = (Contact)contact;
    }

    for (SObject acc : a) {
        for (SObject con : c) {
            returnList.add(Name=acc, Contact=con);
        }            
    }
    
    insert returnList;

    return returnList;            
}