public Integer fibonacci(Integer n) {

    Integer var1 = 1;
    Integer var2 = 1;

    List<Integer> numList = new List<Integer>();
    numList.add(var1);
    numList.add(var2);
    Integer total = numList.size();
    Integer indexNum = total - 1;
    Integer answer = numList[indexNum];
    
    if (n >= 3) {
        var2 = var1 + var2;
        numList.add(var2)
        return answer;
    }
    else {
        return answer;
    }
}