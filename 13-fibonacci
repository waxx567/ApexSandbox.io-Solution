public Integer fibonacci(Integer n) {

    Integer var1 = 1;
    Integer var2 = 1;

    // List<Integer> numList = new List<Integer>();
    // numList.add(var1);
    // numList.add(var2);
    
    if (n == 1 || (n == 2)) {
        return var2;
    }
    else if (n == 3) {
        return var1 + var2;
    }
}