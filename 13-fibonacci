public Integer fibonacci(Integer n) {

    Integer var1 = 1;
    Integer var2 = 1;

    List<Integer> numList = new List<Integer>();
    numList.add(var1);
    numList.add(var2);

    if (n == 1 || (n == 2)) {
        return numList.size()-1;
    }
    else {
        for (Integer i = 2; i < n+1; i++) {
            var1 = var1 + n;
            numList.add(var1)
            var2 = var2 + var1;
            numList.add(var2);
        }
        return numList.size()-1;
    }
}