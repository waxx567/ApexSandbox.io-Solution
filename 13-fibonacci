public Integer fibonacci(Integer n) {

    Integer var1 = 1;
    Integer var2 = 1;

    List<Integer> numList = new List<Integer>();
    numList.add(var1);
    numList.add(var2);

    if (n == 1 || (n == 2)) {
        return numList.size()-1;
    }
    else {
        for (Integer i = 0; i < n; i++) {
            var3 = (numList.size()-2) + (numList.size()-1);
            numList.add(var3):
        }
        return numList.size()-1;
    }
}