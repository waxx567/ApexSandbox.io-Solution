public Integer fibonacci(Integer n) {

    Integer var1 = 1;
    Integer var2 = 1;

    List<Integer> numList = new List<Integer>();
    numList.add(var1);
    numList.add(var2);
    Integer answer = numList.size()-1;
    
    if (n == 1 || (n == 2)) {
        return answer;
    }
    else if {
        var2 = var1 + var2;
        numList.add(var2)
        return answer;
    }
    else {
        return 0;
    }
}