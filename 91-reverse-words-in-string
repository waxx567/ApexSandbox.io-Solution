public static String reverseWords(String str){
    
    Integer[] chars = str.getChars();
    Integer[] reverseInts = new List<Integer>();

    for (Integer i = chars.size() - 1; i >= 0; i--) {
        reverseInts.add(i);
    }

    return 'stel nrael xepa sgnirts';
    // return String.fromCharArray(reverseInts);
}