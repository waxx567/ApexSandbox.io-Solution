public static Integer findLargest(Integer num1, Integer num2, Integer num3) {
	
    Integer answer = 0;
    
    if ((num1 > num2) && (num1 > num3)) {
        answer = num1;
    }
    if ((num2 > num1) && (num2 > num3)) {
        answer = num2;
    }
    if ((num3 > num1) && (num3 > num2)) {
        answer = num3;
    }
    return answer;
}