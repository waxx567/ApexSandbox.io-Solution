public Boolean sameElements(List<Integer> nums1, List<Integer> nums2) {
    if (nums1.size() != nums2.size()) {
        return false;
    }

    for (Integer i : nums1) {
        if (nums2.contains(i) == false) {
            return false;
        }
    }  

    for (Integer i : nums2) {
        if (nums1.contains(i) == false) {
            return false;
        }
    }
    // for (Integer i = 0; i < )

    return true;
}