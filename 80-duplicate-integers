public Boolean containsDuplicates(List<Integer> nums) {
    Set<Integer> set1 = new Set<Integer>(nums);

    for (Integer num : nums) {
        if (set1.contains(num) == true) {
            return false;
        }
        else if (set1.contains(num) == false) {
            Boolean result = set1.add(num);
        }
    }

    return true;
}