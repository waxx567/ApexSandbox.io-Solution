public String convert15to18DigitId(String fifteenDigit)
{
    // Check if the input string is 15 characters long
    if (fifteenDigit.length() != 15) {
        return -1;
    }

    String str15 = Id.valueOf(fifteenDigit); 

    Id eighteenDigit = str15.to18();

    String str18 = String.valueOf(eighteenDigit);

    return str18;
}