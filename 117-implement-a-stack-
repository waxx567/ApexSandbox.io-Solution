public class Stack {
    List<Object> stackList = new List<Object>();
    
    public void push(Object obj) {
        //implement push
        stackList.add(obj);
    }
    
    public Object pop() {
        //implement pop
        Object popObject = stackList.remove(stackList.size()-1);
        return popObject;
    }
    
    public Integer size() {
        //implement size
        Integer listSize = 0;
        for (Integer i = 0; i < stackList.size(); i++) {
            listSize += 1;
        }
        return listSize;
    }
    
    public Object peek(){
        //implement peek
        if (stackList.size() == 0) {
            return stackList.get(-1);
        } else {
            return null;
        }
    }
    
    public Boolean isEmpty() {
        //implement isEmpty
        Boolean empty = false;
        if (stack.size() == 0) {
            empty = true;
        }
        return empty;
    }
}