public List<Integer> evenNumbers(Integer n) {
    List<Integer> returnList = new List<Integer>();
    Integer listInt = 2;
    if (n > 0) {
        returnList.add(listInt);
        for (Integer i = 1; i < n; i++) {
            listInt += 2;
            returnList.add(listInt);
        }
    }
    return returnList;
}