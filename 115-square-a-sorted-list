public static List<Integer> squareList(List<Integer> nums){
    for (Integer i = 0; i < nums.size(); i++) {
        for (Integer num : nums) {
            nums[i] = num * num;
        }
    }
    Integer count = 0;
    Integer p1 = nums[count];
    Integer p2 = nums[count+1];
    Integer temp = 0;
    while (count < nums.size()) {
        if (p1 > p2) {
            temp = p2;
            p2 = p1;
            p1 = temp;
        }
        count++;
    }
    
    return nums;
}